{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "allowJs": true,
    "checkJs": true,
    "noEmit": false,
    "skipLibCheck": true
  },
  "include": [
    "*.ts",
    "*.js"
  ],
 "exclude": ["node_modules", "dist", "eslint.config.js"]
}
